<view class="nim-p2p-msg-receipt-wrapper" wx:if="{{showReadStatus}}">
  <view wx:if="{{p2pMsgRotateDeg == 360}}" class="icon-read-wrapper">
    <icon type="icon-read" size="16" />
  </view>
  <view wx:else class="sector">
    <view class="cover-1" style="transform: rotate({{p2pMsgRotateDeg}}deg)"></view>
    <view class="{{p2pMsgRotateDeg >= 180 ? 'cover-2 cover-3' : 'cover-2'}}"></view>
  </view>
</view>
